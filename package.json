{
  "name": "yanghuan-core-service",
  "version": "1.0.0",
  "description": "API of the Yanghuan eco-system",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist/* && tslint -c tslint.json -p tsconfig.json",
    "build": "tsc && cp -rf package.json dist/package.json && cp -rf config dist/config",
    "test-build": "tsc --noEmit && cp -rf package.json dist/package.json && cp -rf config dist/config",
    "prestart": "npm run build",
    "start": "ts-node .",
    "test": "mocha --exit -r ts-node/register test/**/*.test.ts",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha --exit -r ts-node/register test/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Canterlot-Group/api.git"
  },
  "keywords": [
    "api",
    "streaming",
    "service",
    "internet",
    "radio"
  ],
  "author": "Alicja \"alis\" Lis <alis@krutt.org>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Canterlot-Group/api/issues"
  },
  "homepage": "https://github.com/Canterlot-Group/api#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "morgan": "^1.10.0",
    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.5",
    "sequelize-typescript": "^2.0.0-beta.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.33",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/config": "0.0.36",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/mocha": "^8.0.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.6",
    "@types/pg": "^7.14.5",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "chai": "^4.2.0",
    "mocha": "^8.2.0",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.0",
    "ts-node": "^9.0.0",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  }
}
